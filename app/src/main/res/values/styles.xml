<resources>


    <style name="AppMaterialTheme" parent="SuperMaterialTheme">
        <!-- for below 21 -->


    </style>

    <!-- Base application theme. -->
    <style name="SuperMaterialTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        
        <item name="colorControlHighlight">#C6FF00</item>
        <item name="colorButtonNormal">#FF9E80</item>
        <item name="colorControlActivated">@color/colorAccent</item>
    </style>
    
    
    
    <style name="Mytoolbar" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:textColorSecondary">#FFFFFF</item>
    </style>
    
    <style name="Mypopup" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textColorPrimary">#000000</item>
    </style>


    <style name="MyButton" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">#B2EBF2</item>
        <item name="colorButtonNormal">#E91E63</item>
    </style>



</resources>
